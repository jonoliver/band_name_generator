r = require './random_words'
_ = require('underscore')

module.exports =     
  RandomizeAll: ->
    [
      "with #{r.Adjective()} #{r.Noun()} We #{r.Verb()}" 
      "#{_.sample ['upon', 'among']} the #{r.Noun()} #{r.Verb()}ing"
      "like #{r.Adjective()} #{r.Noun()}"
      "#{r.Verb()}ing the #{r.Adjective()} #{r.Noun()}."
      "#{_.sample ['he', 'she']} #{r.Verb()}s  a #{r.Noun()}" 
      "from the #{r.Adjective()}  #{r.Noun()}"
    ]