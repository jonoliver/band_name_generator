r = require './random_words'
_ = require('underscore')

module.exports =     
  RandomizeAll: ->
    _([
      "with #{r.Adjective()} #{r.Noun()}"
      "we #{r.Verb()}"
      "#{_.sample ['upon', 'among']} the #{r.Noun()} #{r.Gerund()}"
      "like #{r.Adjective()} #{r.Noun()}"
      "#{r.Gerund()} the #{r.Adjective()} #{r.Noun()}"
      "#{_.sample ['he', 'she']} #{r.Verb()}s #{r.An_A r.SingularNoun()}"
      "from the #{r.Adjective()} #{r.Noun()}"
    ]).shuffle()